config wifi-device 'radio0'
	option type 'mac80211'
	option channel '{{ accesspoint_channel }}'
	option hwmode '11g'
	option path 'platform/qca955x_wmac'
	option htmode 'HT40'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid '{{ accesspoint_ssid }}'
	option encryption 'psk2'
	option key '{{ accesspoint_psk }}'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11a'
	option path 'pci0000:00/0000:00:00.0'
	option htmode 'HT40+'
	option channel '{{ accesspoint_channel_5g }}'
	option txpower '16'
	option country 'US'

config wifi-iface
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option ssid '{{ accesspoint_ssid_5g }}'
	option encryption 'psk2'
	option key '{{ accesspoint_psk }}'

